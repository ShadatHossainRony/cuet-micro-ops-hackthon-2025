{
  "name": "delineate-hackathon-challenge-cuet-fest-25-nodejs",
  "type": "module",
  "engines": {
    "node": ">=24.10.0"
  },
  "scripts": {
    "dev": "node --env-file=.env --experimental-transform-types --watch src/index.ts",
    "start": "node --env-file=.env --experimental-transform-types src/index.ts",
    "start:win": "powershell -ExecutionPolicy Bypass -File start.ps1",
    "dev:win": "powershell -ExecutionPolicy Bypass -File dev.ps1",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:e2e": "node --experimental-transform-types scripts/run-e2e.ts",
    "docker:dev": "docker compose -f docker/compose.dev.yml up --build -d",
    "docker:dev:logs": "docker compose -f docker/compose.dev.yml logs -f",
    "docker:dev:down": "docker compose -f docker/compose.dev.yml down",
    "docker:prod": "docker compose -f docker/compose.prod.yml up --build -d"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@hono/node-server": "^1.19.6",
    "@hono/otel": "^1.0.1",
    "@hono/sentry": "^1.2.2",
    "@hono/zod-openapi": "^1.1.5",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@scalar/hono-api-reference": "^0.9.28",
    "hono": "^4.10.8",
    "hono-rate-limiter": "^0.4.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hono/eslint-config": "^2.0.3",
    "@types/node": "^24.10.2",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  }
}
